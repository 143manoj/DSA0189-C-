#include <iostream>
#include <string>
using namespace std;



int main() {
    string username1, username2;

    cin >> username1;

    cin >> username2;

    if (username1 == username2 ) {
        cout << "User name is Valid" << endl;
    } else {
        cout << "User name is Invalid" << endl;
    }

    return 0;
}
