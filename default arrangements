#include <iostream>

int sum(int a, int b = 10, int c = 20, int d = 30) {
    return a + b + c + d;
}

int main() {
    std::cout << "Sum = " << sum(5) << std::endl;
    std::cout << "Sum = " << sum(5, 15) << std::endl;
    std::cout << "Sum = " << sum(5, 15, 25) << std::endl;
    std::cout << "Sum = " << sum(5, 15, 25, 35) << std::endl;
    return 0;
}
